Host *github.com
    User git
    IdentityFile ~/.ssh/id_rsa
    Hostname ssh.github.com
    Port 443

Host fedora
    Hostname fedora.chancez.xyz
    User chance
    IdentityFile ~/.ssh/id_rsa

Host pi
    Hostname pi.home.chancez.xyz
    User chance
    IdentityFile ~/.ssh/id_rsa

Host chance-desktop
    Hostname chance-desktop.home.chancez.xyz
    User chance
    IdentityFile ~/.ssh/id_rsa

Host chance-desktop-wsl
    ProxyJump chance-desktop
    Hostname localhost
    User chance
    Port 2222

Host soma
    Hostname soma.home.chancez.xyz
    User root
    IdentityFile ~/.ssh/id_rsa

Host white
    Hostname white.home.chancez.xyz
    User chance
    IdentityFile ~/.ssh/id_rsa

Host gl synology
    Hostname synology.home.chancez.xyz
    User chance
    IdentityFile ~/.ssh/id_rsa

Host router usg
    Hostname usg.home.chancez.xyz
    User admin
    IdentityFile ~/.ssh/id_rsa

# lima Vms
Match exec "command -v limactl >/dev/null && limactl list -q | grep -q '%h'"
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    ProxyCommand limactl shell %h nc 127.0.0.1 22

Host *
    Include ~/.ssh/config_work
